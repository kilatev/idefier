{
    "name": "Idefier",
    "version": "0.0.67",
    "manifest_version": 2,
    "description": "This app makes brower capable of simple code editor stuff",
    "icons": {
        "16": "images/text-editor.png",
        "128": "images/text-editor.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/background.js",
            "scripts/content_scripts/hilitor.js",
            "scripts/content_scripts/idefier.js",
            "scripts/content_scripts/selector.js"
        ]
    },
    "web_accessible_resources": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/jquery/dist/jquery.min.map"
    ],
    "permissions": [
        "tabs",
        "management",
        "debugger",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "bower_components/jquery/dist/jquery.min.js",
                "scripts/content_scripts/hilitor.js",
                "scripts/content_scripts/idefier.js",
                "scripts/content_scripts/selector.js"
            ],
            "run_at": "document_end"
        }
    ]
}